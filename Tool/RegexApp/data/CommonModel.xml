<?xml version="1.0" encoding="utf-8"?>
<Root>
  <Data>
    <RegexTxt>(13[0-9]|14[5|7]|15[0|1|2|3|5|6|7|8|9]|18[0|1|2|3|5|6|7|8|9])\d{8}</RegexTxt>
    <Describe>匹配手机号码</Describe>
  </Data>
  <Data>
    <RegexTxt>[a-zA-z]+://[^\s]*</RegexTxt>
    <Describe>匹配不带Http:// 的域名</Describe>
  </Data>
  <Data>
    <RegexTxt>\w[-\w.+]*@([A-Za-z0-9][-A-Za-z0-9]+\.)+[A-Za-z]{2,14}</RegexTxt>
    <Describe>匹配邮箱</Describe>
  </Data>
  <Data>
    <RegexTxt>[0-9]</RegexTxt>
    <Describe>匹配所有数字</Describe>
  </Data>
  <Data>
    <RegexTxt>[\u4e00-\u9fa5]+</RegexTxt>
    <Describe>查询所有中文</Describe>
  </Data>
  <Data>
    <RegexTxt>[^\x00-\xff]+</RegexTxt>
    <Describe>匹配双字节字符（包含汉字）</Describe>
  </Data>
  <Data>
    <RegexTxt>([01]?\d|2[0-3]):[0-5]?\d:[0-5]?\d</RegexTxt>
    <Describe>匹配时间（时:分:秒）</Describe>
  </Data>
  <Data>
    <RegexTxt>\d{0,3}\.\d{0,3}\.\d{0,3}\.\d{0,3}</RegexTxt>
    <Describe>匹配IP(IPV4)</Describe>
  </Data>
  <Data>
    <RegexTxt>\d{17}[0-9Xx]|\d{15}</RegexTxt>
    <Describe>匹配身份证号</Describe>
  </Data>
  <Data>
    <RegexTxt>(([0-9]{3}[1-9]|[0-9]{2}[1-9][0-9]{1}|[0-9]{1}[1-9][0-9]{2}|[1-9][0-9]{3})-(((0[13578]|1[02])-(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)-(0[1-9]|[12][0-9]|30))|(02-(0[1-9]|[1][0-9]|2[0-8]))))|((([0-9]{2})(0[48]|[2468][048]|[13579][26])|((0[48]|[2468][048]|[3579][26])00))-02-29)</RegexTxt>
    <Describe>匹配日期(年-月-日)</Describe>
  </Data>
  <Data>
    <RegexTxt>[1-9]\d*</RegexTxt>
    <Describe>匹配正整数</Describe>
  </Data>
  <Data>
    <RegexTxt>-[1-9]\d*</RegexTxt>
    <Describe>匹配负整数</Describe>
  </Data>
  <Data>
    <RegexTxt>(13\d|14[579]|15[^4\D]|17[^49\D]|18\d)\d{8}</RegexTxt>
    <Describe>匹配手机号</Describe>
  </Data>
  <Data>
    <RegexTxt>\d{3}-\d{8}|\d{4}-\d{7}</RegexTxt>
    <Describe>匹配国内电话号码</Describe>
  </Data>
  <Data>
    <RegexTxt>[a-zA-Z][a-zA-Z0-9_]{4,15}</RegexTxt>
    <Describe>匹配帐号(字母开头，允许5-16字节，允许字母数字下划线)</Describe>
  </Data>
  <Data>
    <RegexTxt>\d{4}-\d{1,2}-\d{1,2}</RegexTxt>
    <Describe>匹配日期2(年-月-日)</Describe>
  </Data>
  <Data>
    <RegexTxt>([a-zA-Z]+-?)+[a-zA-Z0-9]+\.[x|X][m|M][l|L]</RegexTxt>
    <Describe>匹配xml 文件</Describe>
  </Data>
  <Data>
    <RegexTxt>\n\s*\r</RegexTxt>
    <Describe>匹配空白行</Describe>
  </Data>
</Root>